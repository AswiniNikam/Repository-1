package com.src;

public class CurruptNumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr= {4,3,4,5,1};
		int i=0;
		while(i<arr.length ) {
			if(arr[i]!=arr[arr[i]-1]&& arr[i]<=arr.length) {
				int other=arr[i]-1;
				int temp=arr[i];
				arr[i]=arr[other];
				arr[other]=temp;
			}else {
			i++;
			}
			
		}

	}

}
